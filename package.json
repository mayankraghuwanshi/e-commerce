{
  "name": "proshop-v2",
  "version": "1.0.0",
  "description": "ecommerce application built with mern stack",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "install:frontend": "cd frontend && npm install",
    "install:backend": "npm install",
    "install:all": "npm run install:backend && npm run install:frontend",
    "build:frontend": "cd frontend && npm run build",
    "build": "npm run install:all && npm run build:frontend",
    "start": "node backend/server.js",
    "server": "nodemon backend/server.js",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node backend/seeder.js",
    "data:destroy": "node backend/seeder.js -d"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kraghu1c/e-commerce.git"
  },
  "author": "Krishank Raghuvanshi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kraghu1c/e-commerce/issues"
  },
  "homepage": "https://github.com/kraghu1c/e-commerce#readme",
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "colors": "^1.4.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.13.2"
  },
  "devDependencies": {
    "concurrently": "^9.1.2",
    "dotenv": "^16.4.7",
    "nodemon": "^2.0.22"
  }
}
